{
  "version": 2,
  "name": "vote-ucao-frontend",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/connexion",
      "destination": "/user/login.html"
    },
    {
      "source": "/inscription",
      "destination": "/user/register.html"
    },
    {
      "source": "/tableau-de-bord",
      "destination": "/user/dashboard.html"
    },
    {
      "source": "/elections",
      "destination": "/user/elections.html"
    },
    {
      "source": "/voter",
      "destination": "/user/vote.html"
    },
    {
      "source": "/candidature",
      "destination": "/user/candidature.html"
    },
    {
      "source": "/mes-candidatures",
      "destination": "/user/mes-candidatures.html"
    },
    {
      "source": "/resultats",
      "destination": "/user/results.html"
    },
    {
      "source": "/profil",
      "destination": "/user/profile.html"
    },
    {
      "source": "/administration",
      "destination": "/admin/adminIndex.html"
    },
    {
      "source": "/administration/tableau-de-bord",
      "destination": "/admin/dashboardAdmin.html"
    },
    {
      "source": "/administration/elections",
      "destination": "/admin/elections.html"
    },
    {
      "source": "/administration/candidats",
      "destination": "/admin/candidats.html"
    },
    {
      "source": "/administration/utilisateurs",
      "destination": "/admin/users.html"
    },
    {
      "source": "/administration/resultats",
      "destination": "/admin/results.html"
    },
    {
      "source": "/administration/statistiques",
      "destination": "/admin/stats.html"
    },
    {
      "source": "/administration/parametres",
      "destination": "/admin/parametre.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/adminIndex.html"
    },
    {
      "source": "/admin/:page",
      "destination": "/admin/:page.html"
    },
    {
      "source": "/user",
      "destination": "/user/login.html"
    },
    {
      "source": "/user/:page",
      "destination": "/user/:page.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}